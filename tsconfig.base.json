{
    "compilerOptions": {
      "target": "esnext",
      "module": "commonjs",
      "lib": ["dom", "esnext"],
      "jsx": "react",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }